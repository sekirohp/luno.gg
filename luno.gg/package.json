{
  "name": "luno.gg",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "generate": "prisma generate",
    "start:prod": "npm run generate && npx prisma db push && npm start",
    "start:voice": "cd voice-server && npm run dev",
    "start:python": "cd python-api && pip install -r requirements.txt && python -m prisma generate --schema=../prisma/schema.prisma && python main.py",
    "start:all": "npm install && npm run build && concurrently \"npm run start:python\" \"npm run start\"",
    "dev:all": "concurrently \"npm run start:python\" \"npm run start:voice\" \"npm run dev\"",
    "lint": "eslint"
  },
  "prisma": {
    "seed": "npx ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "cloudinary": "^2.8.0",
    "lucide-react": "^0.562.0",
    "nanoid": "^5.1.6",
    "next": "16.1.1",
    "next-auth": "^4.24.13",
    "prisma": "^5.22.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "swr": "^2.3.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "concurrently": "^9.0.0"
  }
}
